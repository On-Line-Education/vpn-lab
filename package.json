    "apollo-server-express": "^3.6.5",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.7.1",
    "core.js": "^0.4.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "graphql": "^15.0.0",
    "jkanban": "^1.3.1",
    "papaparse": "^5.3.2",
    "prisma": "^3.11.0",
    "stream": "^0.0.2",
    "ts-node": "^10.9.1",
    "vpnrpc": "^1.0.1",
    "vue": "^3.2.31",
    "vue-flatpickr-component": "^9.0.5",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2",
    "typescript": "^4.6.2",
    "concurrently": "^7.0.0",
    "deepmerge-ts": "^4.0.3",
    "webpack": "^5.70.0"
  },
