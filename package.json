{
  "name": "oedu-vpn-rpc",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "scripts": {
    "serve": "npx vue-cli-service serve",
    "dev": "npx concurrently \"npm run dev:server\" \"npm run dev:serve\"",
    "dev:serve": "npx vue-cli-service serve --mode development",
    "dev:server": "nodemon",
    "dockerstart:local": "npm i && npm run prisma:setup:local && npm run command create:admin admin && npm run dev",
    "dockerstart:dev": "npm i && npm run prisma:setup:dev && npm run command create:admin admin && concurrently \"npm run start\" \"npm run dev:serve\"",
    "dockerstart:prod": "npm i && npm run prisma:setup && concurrently \"npm run start\" \"npm run serve\"",
    "command": "node ./server/Commands/command.cjs",
    "start": "npx ts-node server/index.ts",
    "prisma:setup": " npx prisma migrate deploy",
    "prisma:setup:local": "npx prisma generate && npx prisma migrate dev",
    "prisma:setup:dev": "npx prisma migrate deploy"
  },
  "main": "index.ts",
  "dependencies": {
    "@apollo/client": "^3.6.2",
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@graphql-tools/merge": "^8.2.9",
